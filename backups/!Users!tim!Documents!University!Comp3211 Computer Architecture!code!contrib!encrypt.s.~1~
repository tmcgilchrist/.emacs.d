
1:	li	$r0, (0x08)		; Load start address of input - 1


2:      add	$r0, $r0, $c1		: Increment to next input
3:	ldx	$r2, $r0		; Load input

4:      li	$r1, (0x00)		; Load start address of encryption key
5:	ldr	$r3, $r1		; Load encrytion key
6:	add	$r1, $r1, $c1		; Increment to next part of key


7:      xor	$r2, $r2, $r3
8:	add	$r3, $r2, $c0           ; Implementing formula
9:	lsl	$r3
10:	lsr	$r3
11:	lsl	$r2
12:	xor	$r2, $r2, $r3
13:	ldr	$r3, $r1		; Load encrytion key
14:	add	$r1, $r1, $c1		; Increment to next part of key


15:     xor	$r2, $r2, $r3
16:	add	$r3, $r2, $c0           ; Implementing formula
17:	lsl	$r3
18:	lsr	$r3
19:	lsl	$r2
20:	xor	$r2, $r2, $r3
21:	ldr	$r3, $r1		; Load encrytion key
22:	add	$r1, $r1, $c1		; Increment to next part of key


23:     xor	$r2, $r2, $r3
24:	add	$r3, $r2, $c0           ; Implementing formula
25:	lsl	$r3
26:	lsr	$r3
27:	lsl	$r2
28:	xor	$r2, $r2, $r3
29:	ldr	$r3, $r1		; Load encrytion key
30:	add	$r1, $r1, $c1		; Increment to next part of key


31:     xor	$r2, $r2, $r3
32:	add	$r3, $r2, $c0           ; Implementing formula
33:	lsl	$r3
34:	lsr	$r3
35:	lsl	$r2
36:	xor	$r2, $r2, $r3
37:	ldr	$r3, $r1		; Load encrytion key
38:	add	$r1, $r1, $c1		; Increment to next part of key


39:     xor	$r2, $r2, $r3
40:	add	$r3, $r2, $c0           ; Implementing formula
41:	lsl	$r3
